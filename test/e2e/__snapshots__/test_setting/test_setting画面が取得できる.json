\\x81\\xab\\xe5\\xa4\\xb1\\xe6\\x95\\x97\\xe3\\x81\\x97\\xe3\\x81\\xbe\\xe3\\x81\\x97\\xe3\\x81\\x9f\\xe3\\x80\\x82\");\\n                }\\n              });\\n            });\\n\\n            // \\xe3\\x83\\xa6\\xe3\\x83\\xbc\\xe3\\x82\\xb6\\xe3\\x83\\xbc\\xe8\\xbe\\x9e\\xe6\\x9b\\xb8\\xe5\\x91\\xa8\\xe3\\x82\\x8a\\n            const userDictFileForImport = ref();\\n\\n            const importUserDict = () => {\\n              if (userDictFileForImport.value == undefined) {\\n                throw new Error(\"userDictFileForImport\\xe3\\x81\\x8c\\xe8\\xa6\\x8b\\xe3\\x81\\xa4\\xe3\\x81\\x8b\\xe3\\x82\\x8a\\xe3\\x81\\xbe\\xe3\\x81\\x9b\\xe3\\x82\\x93\\xe3\\x80\\x82\");\\n              }\\n\\n              const reader = new FileReader();\\n              reader.addEventListener(\"load\", async () => {\\n                const params = new URLSearchParams({\\n                  override: true, // \\xe9\\x87\\x8d\\xe8\\xa4\\x87\\xe3\\x81\\x99\\xe3\\x82\\x8b\\xe3\\x82\\xa8\\xe3\\x83\\xb3\\xe3\\x83\\x88\\xe3\\x83\\xaa\\xe3\\x82\\x92\\xe4\\xb8\\x8a\\xe6\\x9b\\xb8\\xe3\\x81\\x8d\\xe3\\x81\\x99\\xe3\\x82\\x8b\\n                });\\n                await fetch(`/import_user_dict?${params}`, {\\n                  method: \"POST\",\\n                  mode: \"same-origin\",\\n                  headers: { \"Content-Type\": \"application/json\" },\\n                  body: reader.result,\\n                });\\n\\n                showToastWithMessage(\"\\xe8\\xbe\\x9e\\xe6\\x9b\\xb8\\xe3\\x82\\x92\\xe3\\x82\\xa4\\xe3\\x83\\xb3\\xe3\\x83\\x9d\\xe3\\x83\\xbc\\xe3\\x83\\x88\\xe3\\x81\\x97\\xe3\\x81\\xbe\\xe3\\x81\\x97\\xe3\\x81\\x9f\\xe3\\x80\\x82\");\\n              });\\n\\n              reader.readAsText(userDictFileForImport.value);\\n            };\\n\\n            // \\xe3\\x83\\x88\\xe3\\x83\\xbc\\xe3\\x82\\xb9\\xe3\\x83\\x88\\n            const toastElem = ref(undefined);\\n            const bootstrapToast = ref(undefined);\\n            const toastMessage = ref(\"\");\\n  